{
  "rpipe": {
    "prefix": "pipe",
    "body": [
      "$0${1:df} <- ${2:df} %>%",
      "\tfilter() %>%",
      "\tselect() %>%",
      "\tmutate() %>%",
      "\tgroup_by() %>%",
      "\tsummarise()"
    ],
    "description": "Insert dplyr pipe"
  },
  "count": {
    "prefix": "count",
    "body": [
      "$0${1:df} %>%",
      "\tcount(${2:variable}, sort = TRUE)"
    ],
    "description": "Insert dplyr count"
  }
}
